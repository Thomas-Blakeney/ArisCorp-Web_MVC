                    e.preventDefault();
                    $(this).next('.sub-alien-menu').slideToggle('slow');
                    $('.fa-alien').toggleClass('fa-spin-90-nota');
                    $('.fa-alien').toggleClass('fa-spin-0-nota');
                });
            });
        </script>
        
        <style>
